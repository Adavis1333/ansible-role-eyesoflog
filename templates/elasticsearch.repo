[elasticsearch-{{ es_version }}]
name=Elasticsearch repository for {{ es_version }} packages
baseurl={{ es_repo_base }}/packages/{{ es_version }}/yum
gpgcheck=1
gpgkey={{ es_repo_base }}/GPG-KEY-elasticsearch
enabled=1
autorefresh=1
type=rpm-md
{% if es_proxy_host is defined and es_proxy_host != '' and es_proxy_port is defined %}
proxy=https://{{ es_proxy_host }}:{{es_proxy_port}}
{% endif %}